{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "JS udacity Project 2 ",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "migrate": "db-migrate --env test up && db-migrate up",
    "test:prepare": "db-migrate db:drop shopping_test && db-migrate db:create shopping_test && db-migrate --env test up",
    "test": "npx tsc && npm run test:prepare && cross-env ENV=test npm run jasmine",
    "jasmine": "jasmine",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "author": "ChienTD2",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/pg": "^7.14.7",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^5.1.6"
  }
}
